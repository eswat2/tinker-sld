{
  "name": "tinker-sld",
  "version": "0.0.740",
  "private": true,
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "autoprefixer": "10.4.24",
    "bumpp": "10.4.0",
    "concurrently": "9.2.1",
    "only-allow": "1.2.2",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "proto-tailwindcss-clrs": "0.0.464",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-solid": "2.11.10"
  },
  "dependencies": {
    "between.js": "0.1.2-fix.2",
    "solid-js": "1.9.11"
  },
  "scripts": {
    "bump": "bumpp patch --commit --tag --push --yes",
    "deploy-app": "cd dist; ln -s ../.vercel; vc --prod",
    "release": "pnpm bump && pnpm build && pnpm deploy-app",
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently \"pnpm:twx --watch\" \"pnpm:start\"",
    "twx": "sh ./.generate",
    "start": "vite",
    "build": "pnpm twx; pnpm format; vite build",
    "serve": "vite preview",
    "spell": "cspell 'src/**/*.{ts,tsx,html,css,md}'",
    "cc": "prettier --check './src/**/*.{ts,tsx,html,css,pcss,md}'",
    "format": "prettier --write './src/**/*.{ts,tsx,html,css,pcss,md}'",
    "open": "open https://tinker-sld.vercel.app"
  },
  "browserslist": [
    "Chrome 74",
    "Firefox 63",
    "Safari 11",
    "Edge 17",
    "Node 10"
  ]
}
